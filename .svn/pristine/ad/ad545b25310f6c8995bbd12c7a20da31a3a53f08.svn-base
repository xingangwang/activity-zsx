} 
.head .rig-head .state .fbth{
	width: 100%;
	display: block;
	float: left;
	position: absolute;
	left: 0;
	top: -11px;
}
.head .rig-head .wuyu .state .mark-rig{
	display: block;
	float: left;
	margin-top: 5px;
	margin-left: 10px;
}
