/*--header开始--*/
#navbar{
	width: 100%;
	height: 54px;
	background: #222222;
}
#navbar .middle-nav{
	max-width:1170px;
	height: 54px;
	margin: 0 auto;
}
#navbar .middle-nav .left-nav{
	max-width: 200px;
	height: 20px;
	float: left;
}
#navbar .middle-nav .left-nav span{
	display: block;
	font-family: "宋体";
	color:#F6F6F6;
	font-size: 18px;
	font-weight:bold;
	line-height: 54px;
	padding-left: 10px;
}
#navbar .middle-nav .right-nav{
	max-width: 80px;
	height: 20px;
	float: left;
	margin-left: 20px;
	margin-top: 17px;
	border-left: 1px solid #ffffff;
}
#navbar .middle-nav .right-nav span{
	font-family: "微软雅黑";
	color:#F6F6F6;
	font-size: 14px;
	text-align: center;
	display: block;
	padding-left: 10px;
}
#content{
	max-width: 1170px;
	margin:  0 auto;
	margin-top: 5%;
}
.middle{
	max-width: 320px;
	padding-left: 10px;
	padding-right:10px;
	margin: 0 auto;
}
#content .main-text{
	max-width: 330px;
	margin: 0 auto;
	
}
#content .main-text span{
	display: block;
	font-family: "微软雅黑";
	color: #58666e;
	font-size: 16px;
	text-align: center;
}
#content .checkbox{
	max-width: 330px;
	height:45px ;
	border:1px solid #bcbcbc;
	border-radius: 3px;
	background: #f5f5f5;
	margin: 0 auto;
	margin-top: 15px;
	position: relative;
	background-image: url(../img/divice.png);
	background-position: 40px center;
	background-repeat:no-repeat;
}
#content .checkbox input{
	border: none;
	background: none;
}
#content .checkbox .name{
	width: 84%;
	height:43px ;
	margin: 0 auto;
	font-family: "微软雅黑";
	color: #acacac;
	font-size: 12px;
	padding-left:50px;
	line-height: 43px \9;
	background-image: url(../img/name.png);
	background-position: 10px center;
	background-repeat:no-repeat;
}
#content .checkbox .phone{
	background: url(../img/phone.png) 10px center no-repeat;
}
#content .checkbox .organ{
	background: url(../img/organ.png) 10px center no-repeat;
}
#content .checkbox .area{
	background: url(../img/area.png) 10px center no-repeat;
}
#content .checkbox .mail{
	background: url(../img/mail.png) 10px center no-repeat;
}
#content .checkbox .password{
	background: url(../img/clock.png) 10px center no-repeat;
}
.checkbox .addres{
	width: 275px;
	height:45px ;
	position: absolute;
	left: 0;
	top: 0;
}
.checkbox .addres img{
	display: block;
	position: absolute;
	right: -20px;
	top:20px;
}
.checkbox .addres a {
	display: block;
	font-family: "微软雅黑";
	color: #acacac;
	font-size: 12px;
	margin-left:50px;
	margin-top: 15px;
}
.checkbox .list-add{
	width:100%;
	height: 200px;
	border: 1px solid #bcbcbc;
	border-top-left-radius:5px;
	border-bottom-left-radius:5px;
	border-radius: 5px;
	background: #FFFFFF;
	position: absolute;
	left: -1px;
	top: -2px;
	z-index: 10;
	overflow: hidden;
}	
.checkbox .list-add .list-left{
	width: 13%;
	height: 200px;
	float: left;
	background: #f5f5f5;
}
.checkbox .list-add .list-left img{
	display: block;
	margin: 0 auto;
	margin-top: 10px;
}
.checkbox .list-add .list-right{
	width: 87%;
	height: 200px;
	float: left;
}
.checkbox .list-add .list-right .bcak{
	background:#F5F5F5 ;
}
.checkbox .list-add .list-right ul{
	height: 200px;
	border-left:1px solid #bcbcbc;
	overflow-x: hidden;
}
.checkbox .list-add .list-right ul li{
	width: 100%;
	height: 30px;
	margin-bottom: 5px;
}
.checkbox .list-add .list-right ul li a{
	display: block;
	text-align: left;
	line-height:30px ;
	color: #ACACAC;
	font-family: "微软雅黑";
	font-size: 14px;
	padding-left: 10px;
}
.checkbox .list-add .list-right ul li:hover{
	background: #F5F5F5;
}
#content .button{
	max-width: 330px;
	height:43px ;
	border-radius: 4px;
	background-color: #23b7e5;
	margin: 0 auto;
	cursor: pointer;
}
#content .button input{
	border: none;
	background:none;
}
#content .button .btn{
	width: 100%;
	display: block;
	font-family: "微软雅黑";
	color: #FFFFFF;
	font-size: 16px;
	margin: 0 auto;
	text-align: center;
	line-height: 45px;
	cursor: pointer;
}
#content .recept{
	max-width:170px ;
	margin: 0 auto;
	margin-top: 10px;
	margin-bottom:10px;
}
#content .recept span{
	display: block;
	font-family: "微软雅黑";
	color: #ff9933;
	font-size: 14px;
}
#content .val-mak{
	max-width: 330px;
	height: 45px;
	margin: 0 auto;
	margin-top: 15px;
	*position: relative;
}
#content .val-mak .entry{
	width: 60%;
	height:42px ;
	border:1px solid #bcbcbc;
	border-radius: 3px;
	background-color: #f5f5f5;
	float:left;
	line-height: 42px \9;
	font-family: "微软雅黑";
	color: #aeaeae;
	font-size: 12px;
	padding-left: 10px;
}	
#content .val-mak input{
	border:none;
	background: none;
}
#content .val-mak .obtain{
	width: 30%;
	height: 45px;
	float: right;
	border: 1px solid #bcbcbc;
	background-color: #f5f5f5;
	border-radius: 3px;
	font-family: "微软雅黑";
	color: #6b6b6b;
	font-size: 14px;
	cursor: pointer;
	*position: absolute;
	*right: 0;
	*top: 0;
}
#content .val-mak .seconds{
	background-color: #eaeaea;
	color: #cccccc;
}
/*-- 设置密码 --*/
.popup{
	position:fixed;
  	top:50%;
  	left:0;
  	width:100%;
  	height:100%;
  	margin-top: -230px;
	z-index: 999;
}
.popup .meng{
	position:absolute;
  	top:-50%;
  	left:0;
  	width:100%;
  	height:100%;
  	margin-top: 230px;
	background: #000000;
	opacity: 0.6;
	z-index: -50;
}
