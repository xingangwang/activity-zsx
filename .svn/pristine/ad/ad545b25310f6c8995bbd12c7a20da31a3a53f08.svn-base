.model-1{
	position: absolute;
	right: 20px;
	top:20px;
}
.chea {
  position: relative;
  display: inline-block;
}
 .fin-top .chea label {
  width: 50px;
  height: 24px;
  background: #e5e6e6;
  position: relative;
  display: inline-block;
  border-radius: 20px;
  -webkit-transition: 0.4s;
  transition: 0.4s;
}
.chea label:after {
  content: '';
  position: absolute;
  width: 18px;
  height: 18px;
  border-radius: 50%;
  left: 2px;
  top: 3px;
  z-index: 2;
  background: #FFFFFF;
  box-shadow: 0  2px 2px rgba(0, 0, 0, 0.2);
  -webkit-transition: 0.4s;
  transition: 0.4s;
}
.chea .cheabox {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 5;
  opacity: 0;
  cursor: pointer;
}     
.chea .cheabox:hover + label:after {
  box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.2), 0 3px 8px 0 rgba(0, 0, 0, 0.15);
}
.chea .cheabox:checked + label:after {
  left: 31px;
}
 
.model-1 .chea .cheabox:checked + label {
  background: #78c93a;
}
.model-1 .chea .cheabox:checked + label:after {
  background: #FFFFFF;
}               
.model-1 span{
	display: block;
	font-family: "微软雅黑";
	font-size: 12px;
	display: block;
	color: #797979;
	float: left;
	margin-top: 4px;
	padding-right:8px;
}

