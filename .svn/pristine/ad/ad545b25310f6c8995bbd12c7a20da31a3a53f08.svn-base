/*上传视频及图片插件样式覆盖*/
.uploadify-queue-item {
	background-color: #F5F5F5;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
	font: 11px Verdana, Geneva, sans-serif;
	margin-top: 5px;
	width: 200px;
	padding: 10px;
	margin-left: 16px;
	float: left;
}

.uploadify-button {
	background-color: #f15d53;
	background-image: linear-gradient(bottom, #f15d53 0%, #f15d53 100%);
	background-image: -o-linear-gradient(bottom, #f15d53 0%, #f15d53 100%);
	background-image: -moz-linear-gradient(bottom, #f15d53 0%, #f15d53 100%);
	background-image: -webkit-linear-gradient(bottom, #f15d53 0%, #f15d53 100%);
	background-image: -ms-linear-gradient(bottom, #f15d53 0%, #f15d53 100%);
	background-image: -webkit-gradient(
			linear,
			left bottom,
			left top,
			color-stop(0, #f15d53),
			color-stop(1, #f15d53)
	);
	background-position: center top;
	background-repeat: no-repeat;
	-webkit-border-radius: 30px;
	-moz-border-radius: 30px;
	border-radius: 0;
	border: 1px solid #993333;
	color: #FFF;
	font: bold 12px Arial, Helvetica, sans-serif;
	text-align: center;
	text-shadow: none;
	width: 100%;
}




.uploadify:hover .uploadify-button {
	background-color: #f15d53;
	background-image: linear-gradient(top, #f15d53 0%, #808080 100%);
	background-image: -o-linear-gradient(top, #f15d53 0%, #808080 100%);
	background-image: -moz-linear-gradient(top, #f15d53 0%, #808080 100%);
	background-image: -webkit-linear-gradient(top, #f15d53 0%, #808080 100%);
	background-image: -ms-linear-gradient(top, #f15d53 0%, #808080 100%);
	background-image: -webkit-gradient(
		linear,
		left bottom,
		left top,
		color-stop(0, #f15d53),
		color-stop(1, #f15d53)
	);
	background-position: center bottom;
}