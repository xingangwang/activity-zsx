	width: 100%;
	height: 48px;
	background-color: #ffffff;
	border-top:1px solid #dbdbdb;
	border-bottom:1px solid #dbdbdb;	
}
.nav-lsit .wai-list{
	width: 1170px;
	margin: 0 auto;
	position: relative;
	z-index: 200;
	cursor:pointer;
}
.nav-lsit .lef-list{
	float: left;
	height: 48px;
}
.nav-lsit .lef-list .head-top{
	float: left;
}
.nav-lsit .lef-list .head-top a{
	display: block;
	height: 19px;
	padding: 0  40px;
	float: left;
	border-left:1px dotted #767676;
	border-right:1px dotted #767676;
	margin-top: 14px;
}
.nav-lsit .lef-list .head-top a img{
	display: block;
	float: left;
}
.nav-lsit .lef-list .head-top a .ye-wu{
	display: block;
	font:14px/20px "微软雅黑";
	color: #333333;
	text-align: left;
	margin-left: 13px;
	float: left;
}
/*-- 下拉菜单  --*/

.bannlist{
	position: absolute;
	left:0px;
	top:0px;
	padding-top: 50px;
	z-index: 999;
}

.bannlist .listone{
	width: 170px;
	height: 190px;
	border-left: 1px solid #e5e2e2;
	float: left;
}
.bannlist .listone .sz-lis{
	width: 170px;
	float: left;
	margin-top: 10px;
	position: relative;
	z-index: 300;
}
.bannlist .listone .sz-lis li{
	float: left;
	width: 75px;
	padding: 5px 0px;
	padding-left: 10px;
}
.bannlist .listone .sz-lis li a{
	display: block;
	font-family: "微软雅黑";
	color: #666666;
